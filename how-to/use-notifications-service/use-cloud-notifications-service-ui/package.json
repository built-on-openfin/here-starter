{
	"name": "openfin-cloud-notifications-service",
	"version": "21.0.0",
	"description": "OpenFin Cloud -- Use Notifications",
	"main": "index.js",
	"scripts": {
		"dos": "node ./scripts/dos.mjs && node ./scripts/kill.mjs",
		"kill": "node ./scripts/kill.mjs",
		"client": "node ./scripts/launch.mjs",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build": "npm run build-client",
		"start": "npx --yes http-server ./public -p 8080 -c-1",
		"setup": "npm install && npm run build",
		"prettier": "prettier --config .prettierrc --write .",
		"prettier-check": "prettier --config .prettierrc --check .",
		"eslint": "eslint . --ext .js,.mjs,.ts",
		"markdownlint": "markdownlint **/*.md --ignore **/node_modules/**",
		"validate": "npm run prettier && npm run eslint && npm run markdownlint"
	},
	"author": "ameet.jayawant@here.io",
	"license": "SEE LICENSE IN LICENSE.MD",
	"dependencies": {
		"@openfin/cloud-notification-core-api": "^0.0.1-alpha.f995a04",
		"@openfin/workspace": "21.0.16",
		"@openfin/workspace-platform": "21.0.16",
		"crypto-browserify": "^3.12.1",
		"csstype": "^3.1.3",
		"dotenv-defaults": "^5.0.2",
		"jsonwebtoken": "^9.0.2",
		"stream-browserify": "^3.0.0",
		"util": "^0.12.5"
	},
	"devDependencies": {
		"@openfin/core": "41.102.4",
		"@openfin/node-adapter": "41.102.4",
		"@types/dotenv-defaults": "^2.0.4",
		"@types/jsonwebtoken": "^9.0.10",
		"eslint": "8.57.1",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "18.0.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsdoc": "48.2.1",
		"eslint-plugin-prettier": "5.1.3",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-simple-import-sort": "12.0.0",
		"eslint-plugin-unicorn": "51.0.1",
		"eslint-plugin-unused-imports": "3.1.0",
		"markdownlint-cli": "0.39.0",
		"prettier": "3.2.5",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.2",
		"vm-browserify": "^1.1.2",
		"webpack": "^5.90.3",
		"webpack-cli": "^5.1.4"
	}
}
